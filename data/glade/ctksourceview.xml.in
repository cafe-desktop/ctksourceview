<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog version="4.0"
               supports="ctkbuilder"
               name="ctksourceview"
               library="ctksourceview-@GSV_API_VERSION@"
               depends="ctk+"
               book="ctksourceview-@GSV_API_VERSION@">
  <glade-widget-classes>
    <glade-widget-class name="CtkSourceView" title="CtkSourceView" generic-name="ctksourceview">
      <properties>
        <property id="auto-indent" default="False" />
        <property id="highlight-current-line" default="False" />
        <property id="indent-on-tab" default="True" />
        <property id="insert-spaces-instead-of-tabs" default="False" />
        <property id="right-margin-position" default="80" />
        <property id="show-line-marks" default="False" />
        <property id="show-line-numbers" default="False" />
        <property id="show-right-margin" default="False" />
        <property id="smart-home-end" default="Disabled">
          <displayable-values>
            <value id="CTK_SOURCE_SMART_HOME_END_DISABLED" name="Disabled" />
            <value id="CTK_SOURCE_SMART_HOME_END_BEFORE" name="Before" />
            <value id="CTK_SOURCE_SMART_HOME_END_AFTER" name="After" />
            <value id="CTK_SOURCE_SMART_HOME_END_ALWAYS" name="Always" />
          </displayable-values>
        </property>
        <property id="tab-width" default="8" />
      </properties>
    </glade-widget-class>
    <glade-widget-class name="CtkSourceMap" title="CtkSourceMap" generic-name="ctksourcemap">
    </glade-widget-class>
    <glade-widget-class name="CtkSourceStyleSchemeChooserWidget" title="CtkSourceStyleSchemeChooserWidget" generic-name="ctksourcestyleschemechooserwidget">
    </glade-widget-class>
    <glade-widget-class name="CtkSourceStyleSchemeChooserButton" title="CtkSourceStyleSchemeChooserButton" generic-name="ctksourcestyleschemechooserbutton">
    </glade-widget-class>
  </glade-widget-classes>

  <glade-widget-group name="CtkSourceView" title="CtkSourceView">
    <glade-widget-class-ref name="CtkSourceView" />
    <glade-widget-class-ref name="CtkSourceMap" />
    <glade-widget-class-ref name="CtkSourceStyleSchemeChooserWidget" />
    <glade-widget-class-ref name="CtkSourceStyleSchemeChooserButton" />
  </glade-widget-group>
</glade-catalog>
